<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:toast="http://azerothian.io/schema/toast-bpmn"
                  id="Definitions_6"
                  targetNamespace="http://example.com/invalid">
  <bpmn:process id="InvalidTypeMismatch" name="Invalid Type Mismatch" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:serviceTask id="Task_A" name="Task A">
      <bpmn:extensionElements>
        <toast:TaskConfig chainEventName="type.taskA" inputType="UnregisteredTypeA" outputType="UnregisteredTypeB" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_B" name="Task B">
      <bpmn:extensionElements>
        <toast:TaskConfig chainEventName="type.taskB" inputType="UnregisteredTypeC" outputType="UnregisteredTypeD" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_3</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_A" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_A" targetRef="Task_B" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_B" targetRef="EndEvent_1" />
  </bpmn:process>
</bpmn:definitions>
