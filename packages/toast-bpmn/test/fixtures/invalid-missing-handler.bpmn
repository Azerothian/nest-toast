<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:toast="http://azerothian.io/schema/toast-bpmn"
                  id="Definitions_5"
                  targetNamespace="http://example.com/invalid">
  <bpmn:process id="InvalidMissingHandler" name="Invalid Missing Handler" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Start">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:serviceTask id="Task_NoChain" name="No Chain Event">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:endEvent id="EndEvent_1" name="End">
      <bpmn:incoming>Flow_2</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_NoChain" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_NoChain" targetRef="EndEvent_1" />
  </bpmn:process>
</bpmn:definitions>
